stock LAC_EventsActive(event)
{
	switch(event)
	{
		case 1: // Chrismas
		{
			LAC_EventChristmasState(true);
		}
	}
}

PlaySongLoginEvent(playerid)
{
	switch(ServerGeneral.Info[@EventActive])
	{
		case 0:
		{
			PlayAudioStreamForPlayer(playerid, "https://dl.dropboxusercontent.com/s/2lruhz9ep18txpa/home2.mp3");
		}
		case 1:
		{
			PlayAudioStreamForPlayer(playerid, "https://dl.dropboxusercontent.com/s/804de1p18sqjym8/navidad.mp3");
		}
	}
}


